{
    "name": "tempest/command-bus",
    "description": "A command bus component designed to dispatch commands to their respective handlers.",
    "require": {
        "php": "^8.3",
        "tempest/core": "^v1.0.0-alpha.3",
        "tempest/container": "^v1.0.0-alpha.3"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Tempest\\CommandBus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\CommandBus\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
